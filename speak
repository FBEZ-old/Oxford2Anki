function getDeviceByResolution() {
var res = null;
if (window.matchMedia("(max-width: 761px)").matches)
res = "mobile";
else if (window.matchMedia("(min-width: 762px) and (max-width: 948px)").matches)
res = "tablet";
else if (window.matchMedia("(min-width: 1240px)").matches)
res = "hd";
else
res = "desktop";
return res;
}
var curResolution = getDeviceByResolution();
